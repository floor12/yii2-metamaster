{
  "name": "floor12/yii2-metamaster",
  "description": "This Yii2 component helps to organize page title, meta tags and `og` and `twitter` open graph tags in with a fiew lines of code.",
  "type": "yii2-component",
  "minimum-stability": "stable",
  "keywords": [
    "opengraph",
    "meta",
    "seo",
    "yii2"
  ],
  "require": {
    "php": ">=7.1.0",
    "yiisoft/yii2": "^2.0."
  },
  "require-dev": {
    "phpunit/phpunit": ">=7.0"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "floor12",
      "email": "floor12@floor12.net",
      "role": "Developer"
    }
  ],
  "autoload": {
    "psr-4": {
      "floor12\\metamaster\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "floor12\\metamaster\\tests\\": "tests"
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ]
}
